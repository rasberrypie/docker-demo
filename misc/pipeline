@Library("pipeline-shared-lib")  _
def commit_id
pipeline {
    agent any
    stages {
        stage("Checkout") {
            steps{
                script {
                    checkout scm
                    sh "git rev-parse --short HEAD > .git/commit-id"                        
                    commit_id = readFile('.git/commit-id').trim()
                    sh "echo ${commit_id}"
                }
            }
            
        }
    }
}
